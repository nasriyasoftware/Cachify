{
    "preset": "ts-jest",
    "transform": {
        "^.+\\.ts?$": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "/tests/.*\\.(test|spec)?\\.(ts|tsx)$",
    "moduleFileExtensions": [
        "ts",
        "js",
        "json",
        "tsx",
        "jsx",
        "node"
    ],
    "setupFilesAfterEnv": [
        "<rootDir>/tests/setup/jest.loadEnv.ts",
        "<rootDir>/tests/setup/jest.redis.ts"
    ]
}